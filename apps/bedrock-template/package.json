{
  "name": "apps-website-template",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.server": "qwik check-client src dist && vite build -c adapters/bun/vite.config.ts",
    "serve": "bun server/entry.bun.js",
    "start": "bun run serve",
    "dev": "vite --mode ssr",
    "typecheck": "tsc --noEmit",
    "lint": "bunx @biomejs/biome check src scripts package.json tsconfig.json vite.config.ts adapters/bun/vite.config.ts",
    "format": "bunx @biomejs/biome format --write src scripts package.json tsconfig.json vite.config.ts adapters/bun/vite.config.ts",
    "test": "bun test",
    "db:migrate": "bun run scripts/migrate.ts",
    "db:seed": "bun run scripts/seed.ts",
    "worker": "bun run scripts/worker.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/s3-request-presigner": "^3.990.0",
    "@builder.io/qwik": "^1.19.0",
    "@builder.io/qwik-city": "^1.19.0",
    "@fontsource-variable/space-grotesk": "^5.2.10",
    "better-auth": "^1.4.18",
    "clsx": "^2.1.1",
    "kysely": "^0.28.11",
    "kysely-postgres-js": "^3.0.0",
    "pg-boss": "^12.11.2",
    "postgres": "^3.4.8",
    "tailwind-merge": "^3.4.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/bun": "^1.3.9",
    "@types/node": "^22",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
